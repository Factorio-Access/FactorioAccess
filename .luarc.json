{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "runtime": {
    "version": "Lua 5.2",
    "path": [
      "?.lua",
      "?/init.lua",
      "scripts/?.lua"
    ],
    "pathStrict": false
  },
  "workspace": {
    "library": [
      "/mnt/c/Users/Austin/AppData/Roaming/Code/User/workspaceStorage/1eb01252218e6ecbe7430012c0965bd2/justarandomgeek.factoriomod-debug/sumneko-3rd/factorio/library"
    ],
    "checkThirdParty": false,
    "ignoreDir": [
      ".vscode",
      ".git"
    ]
  },
  "diagnostics": {
    "enable": true,
    "globals": [
      "players",
      "printout",
      "get_selected_ent",
      "direction_lookup",
      "cursor_highlight",
      "sync_build_cursor_graphics",
      "get_direction_of_that_from_this",
      "clear_obstacles_in_rectangle",
      "ENT_TYPES_YOU_CAN_BUILD_OVER"
    ],
    "disable": [
      "lowercase-global",
      "deprecated",
      "need-check-nil",
      "redefined-local"
    ],
    "severity": {
      "unused-local": "Information",
      "undefined-global": "Warning",
      "undefined-field": "Information",
      "redundant-parameter": "Information",
      "cast-local-type": "Warning",
      "assign-type-mismatch": "Information"
    }
  },
  "format": {
    "enable": false
  },
  "telemetry": {
    "enable": false
  },
  "hint": {
    "enable": true,
    "setType": false
  }
}